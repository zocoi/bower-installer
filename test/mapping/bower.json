{
    "name": "sf2",
    "version": "2.0.0",
    "main": "src/main/app.js",
    "dependencies": {
        "jquery-ui" : "latest",
        "ember-bootstrap": "https://github.com/emberjs-addons/ember-bootstrap/releases/download/01c440e23a4772f5caabe9d8e27e0b6ac9b35c6c/ember-bootstrap.js",
        "ember-easyForm": "https://raw.github.com/dockyard/ember-builds/master/easyForm/ember-easyForm-0.3.2.js"
    },
    "install": {
        "path": "build/src",
        "sources": {
            "jquery-ui" : {
              "mapping": [
                {"bower_components/jquery-ui/ui/jquery-ui.js": "jquery-ui.js"},
                {"bower_components/jquery-ui/ui/minified/jquery-ui.min.js": "jquery-ui-min-new-name.js"}
              ]
            },
            "ember-bootstrap": {
              "mapping": [
                {"bower_components/ember-bootstrap/index.js": "ember-bootstrap.js"}
              ]
            },
            "ember-easyForm": {
              "mapping": [
                {"bower_components/ember-easyForm/index.js": "ember-easyForm.js"}
              ]
            }
        }
    }
}